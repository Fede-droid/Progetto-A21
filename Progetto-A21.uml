          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_w3wY0AWnEeySvasyWs6sIw" name="run" specification="_wm3dIAWnEeySvasyWs6sIw">
            <language>java</language>
            <body>double previous = System.nanoTime(); 
		double delta = 0.0;
		double fps = 100.0;
		double ns = 1e9/fps; // numero di nano sec per fps
		gameStatus = true;
		
		//switchare off/on
		//if (mainMusic.isMusicOn()) mainMusic.playMusic(MusicTypes.LOOP);
		
		while (gameStatus) {
			double current = System.nanoTime();
			
			double elapsed = current - previous;
			previous = current;
			delta += elapsed;

				while (delta >= ns) {
				   update();	
				   delta -= ns;
				}
			render();
		}
	</body>
